{
  "name": "readme-frontend-tests",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "open": "cypress open",
    "run-tests": "cypress run",
    "clear-reports": "rm -rf allure-results && rm -rf allure-report",
    "generate-report": "allure generate allure-results --clean -o allure-report",
    "open-report": "allure open allure-report",
    "test": "npm run clear-reports && npm run run-tests && npm run generate-report && npm run open-report",
    "cy:parallel": "npm run clear-reports && cypress-parallel -s run-tests -t 2 -d ./cypress/e2e && npm run generate-report && npm run open-report"
  },
  "dependencies": {
    "cypress-file-upload": "^5.0.8",
    "cypress-parallel": "^0.14.0",
    "file-saver": "^2.0.5"
  },
  "devDependencies": {
    "allure-commandline": "^2.27.0",
    "allure-cypress": "^2.15.0",
    "cypress": "^13.7.1"
  }
}
